<!DOCTYPE html>
<html {{ site.language_attributes }}>
<head>
    <meta charset="utf-8">
        <title>
            {% if wp_title %}
                {{ wp_title }} - {{ site.name }}
            {% else %}
                {{ site.name }}
            {% endif %}
        </title>
    <meta name="description" content="{{ site.description }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="{{ site.theme.link }}/static/css/uikit.min.css" />
    <link rel="stylesheet" href="{{ site.theme.link }}/style.css" type="text/css" media="screen" />
    <script src="{{ site.theme.link }}/static/js/uikit.min.js"></script>
    <script src="{{ site.theme.link }}/static/js/uikit-icons.min.js"></script>

    {{function('wp_head')}}
